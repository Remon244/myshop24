@model IEnumerable<Category>
<h3>Categories </h3>
<table> 
    <tr>
        <td>Name </td>
        <td>Descraption </td>
        <td>Date Created </td>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Name </td>
            <td> @item.Description</td>
            <td>@item.CreatedDate </td>
        </tr>
    }
</table>